{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"textdomain": "poocommerce",
	"name": "poocommerce/all-products",
	"title": "All Products",
	"category": "poocommerce",
	"keywords": [
		"PooCommerce"
	],
	"description": "Display products from your store in a grid layout.",
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false,
		"multiple": false,
		"inserter": false
	},
	"attributes": {
		"columns": {
			"type": "number"
		},
		"rows": {
			"type": "number"
		},
		"alignButtons": {
			"type": "boolean"
		},
		"contentVisibility": {
			"type": "object"
		},
		"orderby": {
			"type": "string"
		},
		"layoutConfig": {
			"type": "array"
		},
		"isPreview": {
			"type": "boolean",
			"default": false
		}
	}
}
