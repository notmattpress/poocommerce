{
	"name": "poocommerce/add-to-cart-with-options-variation-selector-attribute-options",
	"title": "Variation Selector: Attribute Options (Beta)",
	"description": "Display the attribute options associated with a variable product.",
	"category": "poocommerce-product-elements",
	"keywords": [ "PooCommerce" ],
	"ancestor": [
		"poocommerce/add-to-cart-with-options-variation-selector-attribute"
	],
	"attributes": {
		"optionStyle": {
			"type": "string",
			"enum": [ "pills", "dropdown" ],
			"default": "pills"
		},
		"autoselect": {
			"type": "boolean",
			"default": false
		},
		"disabledAttributesAction": {
			"type": "string",
			"enum": [ "disable", "hide" ],
			"default": "disable"
		}
	},
	"textdomain": "poocommerce",
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"supports": {
		"inserter": false,
		"interactivity": true
	},
	"usesContext": [
		"poocommerce/attributeId",
		"poocommerce/attributeName",
		"poocommerce/attributeTerms"
	],
	"style": "file:../poocommerce/add-to-cart-with-options-variation-selector-attribute-options-style.css"
}
