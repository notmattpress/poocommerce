{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "poocommerce/product-gallery-large-image",
	"title": "Large Image",
	"description": "Display the Large Image of a product.",
	"category": "poocommerce",
	"keywords": [ "PooCommerce" ],
	"usesContext": [ "postId", "hoverZoom", "fullScreenOnClick" ],
	"supports": {
		"interactivity": true
	},
	"allowedBlocks": [
		"poocommerce/product-image",
		"poocommerce/product-sale-badge",
		"poocommerce/product-gallery-large-image-next-previous"
	],
	"textdomain": "poocommerce",
	"ancestor": [ "poocommerce/product-gallery" ],
	"viewScriptModule": "poocommerce/product-gallery-large-image",
	"editorStyle": "file:../poocommerce/product-gallery-large-image-editor.css"
}
